{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": [
            false
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "no-empty-interface": false,
        "no-unused-variable": true,
        "trailing-comma": [
            true,
            "never"
        ]
    },
    "rulesDirectory": [],
    "arrow-parens": false
}